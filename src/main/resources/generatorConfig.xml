<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>
    <classPathEntry location="H:/Java/jar/mysql-connector-java-5.1.44-bin.jar"/>
    <context id="context1">
        <!-- 是否生成注释 -->
        <commentGenerator>
            <property name="suppressDate" value="true"/>
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>
        <!-- 数据库连接 -->
        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://localhost:3306/test"
                        userId="root" password="root" />

        <!-- 生成的包名和工程名 -->
        <javaModelGenerator targetPackage="com.n5x.model.user" targetProject="src/main/java"/>
        <sqlMapGenerator targetPackage="com.n5x.model.user" targetProject="src/main/resources" />
        <javaClientGenerator targetPackage="com.n5x.mapper.user" targetProject="src/main/java" type="XMLMAPPER" />

        <!-- 数据库表 -->
        <table  tableName="USER_LEVEL" domainObjectName="UserLevel" >
            <!-- 设置别名 -->
            <!--
            <columnOverride column="ds_id" property="dsId" />
            <columnOverride column="ds_name" property="dsName" />
            <columnOverride column="dt_name" property="dtName" />
            <columnOverride column="login_name" property="loginName" />
            <columnOverride column="login_pwd" property="loginPwd" />
             -->
        </table>
    </context>
</generatorConfiguration>